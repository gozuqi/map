
  <div id="map" style="width:620px; height:400px"></div>
  <script type="text/javascript">
    function initMap() {
      var opts = {
        zoom: 15,
        center: new google.maps.LatLng(35.66236, 139.760343)
      };
      var map = new google.maps.Map(document.getElementById("map"), opts);
    }
    function initialize() {
      if (GBrowserIsCompatible()) {
        var map = new GMap2(document.getElementById("map_canvas"));
        map.setCenter(new GLatLng(35.681379,139.765577), 13);
        var directions = new GDirections(map, document.getElementById('route'));
        directions.load('from: 東京駅 to: 渋谷駅', {locale: 'ja_JP'});
      }
    }
  </script>

  <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBYibp6uMIbDm8tng2JfGd0KSWhBucdluQ&callback=initMap&libraries=places">
  </script>

  <body onload="initialize()" onunload="GUnload()">
    <div id="map_canvas" style="width: 500px; height: 300px"></div>
    <div id="route" style="width: 500px; height: 200px;overflow: scroll"></div>
  </body>





